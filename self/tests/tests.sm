let
    display = /r.match r {
        ["Passed", #_] in [],
        [] in [],
        [[...#nested], ...#rest] in [...display([...nested]), ...display(rest)],
        #f in f
    }
in
    match display([
        import "token.test.sm",
        import "lexer.test.sm",
        import "ast-string.test.sm",
        import "parser.test.sm"
    ]) {
        [] in "All tests passed",
        #x in x
    }
